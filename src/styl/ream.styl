.container
  margin 0
  padding 0
  .slide
    margin 0
    padding 0
    position relative
    background #ffffff
    z-index 100000
  .slide.shadow
    -webkit-box-shadow 0px 9px 20px 2px rgba(34,34,34,1);
    -moz-box-shadow 0px 9px 20px 2px rgba(34,34,34,1);
    box-shadow 0px 9px 20px 2px rgba(34,34,34,1);
  .slide:nth-child(odd)
    background #00CED1
  .slide:nth-child(even)
    background #FF7F50